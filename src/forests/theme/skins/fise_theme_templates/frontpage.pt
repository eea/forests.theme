<tal:block tal:define="foo python:request.set('disable_plone.leftcolumn', 1);
        bar python:request.set('disable_plone.rightcolumn', 1);"/>
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
         i18n:domain="plone.z3cform"
         metal:use-macro="context/main_template/macros/master">

<body>

<metal:cssslot fill-slot="style_slot">
    <link rel="stylesheet" tal:attributes="href string:${portal_url}/slick.css">
    <link rel="stylesheet" tal:attributes="href string:${portal_url}/slider.css">
</metal:cssslot>
<metal:javascriptslot fill-slot="javascript_head_slot">
    <script type="text/javascript" tal:attributes="src string:${portal_url}/jquery.slick.js"></script>
    <script type="text/javascript" tal:attributes="src string:${portal_url}/slider_init.js"></script>
</metal:javascriptslot>

<metal:top metal:fill-slot="portal_top">
    <div id="highlights-slider">
        <div class="highlight-placeholder">
            <img tal:attributes="src string:${portal_url}/placeholder.jpg" src="/Plone/placeholder.jpg"/>
        </div>
        <div class="header-bottom-section">

        </div>
        <div class="header-logos-wrapper">
            <div class="header-logos">

            </div>
        </div>
        <div id="highlights-slider-content">

            <div class="highlight-photos">
                <picture class="highlight-photo current">
                    <img tal:attributes="src string:${portal_url}/forest1.jpg" src="/Plone/1-md-sm.jpg" />
                </picture>
                <picture class="highlight-photo" style="display: none; ">
                    <img tal:attributes="src string:${portal_url}/forest2.jpg" src="/Plone/2-md-sm.jpg" />
                </picture>
                <picture class="highlight-photo" style="display: none; ">
                    <img tal:attributes="src string:${portal_url}/forest3.jpg" src="/Plone/3-md-sm.jpg" />
                </picture>
            </div>
            <div class="slider-highlights">
                <div class="highlights" id="hlslider-slides">
                    <div class="highlight">
                        <h1 class="highlight-description">Welcome to FISE â€“ the entry point to information on European forest issues in support of knowledge based forest governance and sustainable use and management of forests</h1>
                    </div>
                    <div class="highlight" style="display: none; ">
                        <h1 class="highlight-description">Accurate and timely information of the state of forests ecosystems and their resources is essential to achieve sustainable bioeconomy</h1>
                    </div>
                    <div class="highlight" style="display: none; ">
                        <h1 class="highlight-description">A comprehensive knowledge base is our most valuable resource</h1>
                    </div>

                </div>
                <div id="slider-nav"><span class="slider-prev" id="hlslider-prev"></span> <span class="slider-next" id="hlslider-next"></span></div>
            </div>
        </div>
    </div>
    <style>
    </style>
</metal:top>

<metal:content metal:fill-slot="content">
    <div class="frontpage-content clearfix">
        <object type="image/svg+xml" data="leaf.svg" class="logo">
        </object>
    </div>
</metal:content>
</body>
</html>
