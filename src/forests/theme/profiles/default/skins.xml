<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="FISETheme">

 <object name="fise_theme_images"
    meta_type="Filesystem Directory View"
    directory="forests.theme:skins/fise_theme_images"/>
 <object name="fise_theme_templates"
    meta_type="Filesystem Directory View"
    directory="forests.theme:skins/fise_theme_templates"/>
 <object name="fise_theme_styles"
    meta_type="Filesystem Directory View"
    directory="forests.theme:skins/fise_theme_styles"/>
 <object name="fise_theme_js"
    meta_type="Filesystem Directory View"
    directory="forests.theme:skins/fise_theme_js"/>
 <object name="fise_theme_overrides"
    meta_type="Filesystem Directory View"
    directory="forests.theme:skins/fise_theme_overrides"/>


 <skin-path name="FISETheme" based-on="Sunburst Theme">
  <layer name="fise_theme_images"
     insert-after="custom"/>
  <layer name="fise_theme_templates"
     insert-after="fise_theme_images"/>
  <layer name="fise_theme_styles"
     insert-after="fise_theme_templates"/>
  <layer name="fise_theme_js"
     insert-after="fise_theme_styles"/>
  <layer name="fise_theme_overrides"
     insert-after="custom"/>
 </skin-path>

</object>
